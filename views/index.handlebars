{{#each birthday }}
<div class="birthday-item">
  <ul>
      <li>{{this.name}}</li>
      <li>{{this.date}}</li>
      <a href="/birthday/delete/{{ _id }}">Delete</a>
      <a href="/birthday/show/{{ _id}}">View Details</a>

  </ul>
</div>
{{/each}}
<a class="button" href="/birthday/new">Add New Birthday</a>
